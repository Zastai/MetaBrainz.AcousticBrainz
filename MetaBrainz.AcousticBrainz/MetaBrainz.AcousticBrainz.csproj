<?xml version="1.0" encoding="utf-8"?>
<Project>

  <Sdk Name="MetaBrainz.Build.Sdk" Version="3.1.0" />

  <PropertyGroup>
    <Authors>Zastai</Authors>
    <Title>AcousticBrainz Web Service Client Library</Title>
    <Description>This package provides classes for accessing the AcousticBrainz API (v1).</Description>
    <PackageCopyrightOwners>Tim Van Holder</PackageCopyrightOwners>
    <PackageCopyrightYears>2020, 2021, 2022, 2023</PackageCopyrightYears>
    <PackageRepositoryName>MetaBrainz.AcousticBrainz</PackageRepositoryName>
    <PackageTags>AcousticBrainz music metadata</PackageTags>
    <Version>1.0.0-pre</Version>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyIsComVisible>false</AssemblyIsComVisible>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" IncludeAssets="compile" PrivateAssets="all" />
    <PackageReference Include="MetaBrainz.Common.Json" />
  </ItemGroup>

</Project>
